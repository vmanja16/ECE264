test 22 :: implode(...)

   int len = 6
   char * * strArr = malloc(len * sizeof(char*));
   strArr[0] = strdup("1");
   strArr[1] = strdup("2");
   strArr[2] = strdup("3");
   strArr[3] = strdup("4");
   strArr[4] = strdup("5");
   strArr[5] = strdup("6");
   const char * glue = ", ";
   char * s = implode(strArr, len, glue);
   // s is "1, 2, 3, 4, 5, 6"
   (pass.)

test 23 :: sortStringArray(...)

   int len = 3
   char * * strArr = malloc(len * sizeof(char*));
   strArr[0] = strdup("lady");
   strArr[1] = strdup("beatle");
   strArr[2] = strdup("brew");
   sortStringArray(strArr, len);
   // strArr[0] is "beatle"
   // strArr[1] is "brew"
   // strArr[2] is "lady"
   (pass.)

