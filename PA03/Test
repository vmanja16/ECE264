   strArr[29] = strdup("");
   strArr[30] = strdup("Twas brillig, and the slithy toves");
   strArr[31] = strdup("Did gyre and gimble in the wabe;");
   strArr[32] = strdup("All mimsy were the borogoves,");
   strArr[33] = strdup("And the mome raths outgrabe.");
